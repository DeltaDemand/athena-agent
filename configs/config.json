{
    "Agent": {
        "group": "athena",
        "name": "agent",
        "checkAlive":120,
        "pause": false
    },
    "ReportServer":{
        "ip":"athena-server",
        "port": "8888"
    },
    "Etcd": {
        "apply": true,
        "endPoints": ["112.74.60.132:2379","localhost:2379"],
        "dialTimeout": 5
    },
    "cpu_rate": {
    "run": true,
    "reportInterval": 60
    },
    "memory_used": {
        "run": true,
        "samplingInterval": 1,
        "reportInterval": 60
    },
    "disk_used": {
        "run": true,
        "reportInterval": 60
    },
    "cpu_mem": {
        "run": true,
        "samplingInterval": 1,
        "reportInterval": 60,
        "cpuPercentThreshold": 50.0,
        "memPercentThreshold": 80.0
    }
}


