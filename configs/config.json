{
    "Agent": {
        "send_error_limit": 10,
        "pause": false
    },
    "ReportServer":{
        "check_alive":120,
        "ip":"athena-server",
        "port": "8888"
    },
    "Etcd": {
        "apply": true,
        "config_server": "config-s01",
        "agent_group": "g01",
        "agent_name": "A01",
        "end_points": ["112.74.60.132:2379"],
        "dial_timeout": 5
    },
    "cpu_rate": {
        "run": true,
        "report_interval": 60
    },
    "memory_used": {
        "run": true,
        "sampling_interval": 1,
        "report_interval": 60
    },
    "disk_used": {
        "run": true,
        "report_interval": 60
    },
    "cpu_mem": {
        "run": true,
        "sampling_interval": 1,
        "report_interval": 60,
        "cpu_percent_threshold": 70.0,
        "mem_percent_threshold": 50.0
    }
}
