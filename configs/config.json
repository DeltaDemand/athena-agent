{
    "Agent": {
        "checkAlive":120
    },
    "ReportServer":{
        "ip":"athena-server",
        "port": "8888"
    },
    "CpuSampling": {
        "run": true,
        "reportInterval": 60
    },
    "MemSampling": {
        "run": true,
        "samplingInterval": 1,
        "reportInterval": 60
    },
    "DiskSampling": {
        "run": true,
        "reportInterval": 60
    },
    "CpuMem": {
        "run": true,
        "samplingInterval": 1,
        "reportInterval": 60,
        "cpuPercentThreshold": 50.0,
        "memPercentThreshold": 80.0
    }
}


